%h1 error #{@error.fei.wfid}/#{@error.fei.expid}

.options
  %ul
    %li
      %form{ :method => 'post' }
        %input{ :type => 'hidden', :name => '_method', :value => 'delete' }
        %input{ :type => 'submit', :value => 'replay' }
        %span.description DELETE /_ruote/errors/#{@error.fei.wfid}/#{@error.fei.expid}

%ul
  %li id: #{@error.fei.inspect}
  %li process: #{link_to @process}
  %li message: #{@error.message}

